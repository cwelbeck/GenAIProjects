   "source": [
    "# Finance A/B Testing: Investment Newsletter Subject Line Effectiveness\n",
    "\n",
    "This notebook analyzes whether personalized subject lines in investment newsletters lead to higher click-through rates compared to generic ones."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "from scipy import stats\n",
    "np.random.seed(42)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Simulate A/B test on subject lines\n",
    "n = 1000\n",
    "data = pd.DataFrame({\n",
    "    'user_id': np.arange(n),\n",
    "    'group': np.random.choice(['personalized', 'generic'], size=n),\n",
    "    'clicked': np.random.binomial(1, p=0.1, size=n)\n",
    "})\n",
    "# Personalized group has higher CTR\n",
    "data.loc[data['group'] == 'personalized', 'clicked'] = np.random.binomial(1, p=0.18, size=(data['group'] == 'personalized').sum())\n",
    "data.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Summary statistics\n",
    "summary = data.groupby('group').agg({\n",
    "    'user_id': 'count',\n",
    "    'clicked': ['mean', 'sum']\n",
    "})\n",
    "summary.columns = ['users', 'click_rate', 'total_clicks']\n",
    "summary"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# T-test\n",
    "personalized = data[data['group'] == 'personalized']['clicked']\n",
    "generic = data[data['group'] == 'generic']['clicked']\n",
    "t_stat, p_val = stats.ttest_ind(personalized, generic)\n",
    "print(f"T-statistic: {t_stat:.4f}, P-value: {p_val:.4f}")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Visualization\n",
    "sns.barplot(data=data, x='group', y='clicked', ci='sd')\n",
    "plt.title('Click-Through Rate by Subject Line Type')\n",
    "plt.ylabel('CTR')\n",
    "plt.xlabel('Subject Line')\n",
    "plt.show()"
   ]
  }
